{
    "service": {
        "name": "{{ inventory_hostname|regex_replace('\d+', '') }}",
        "id": "{{ inventory_hostname }}",
        "tags": [
            "web"
        ],
        "port": 8000,
        "checks": [{
            "id": "{{inventory_hostname}}-web-ping",
            "http": "http://localhost:8000/_haproxychk",
            "interval": "15s"
          }]
    }
}  
