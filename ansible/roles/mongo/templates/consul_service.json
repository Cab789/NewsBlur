{
    "service": {
        "name": "db-mongo",
        "id": "{{ inventory_hostname }}",
        "tags": [
            "db"
        ],
        "port": 27017,
        "checks": [{
            "id": "mongo-ping",
            "http": "http://{{ ansible_ssh_host }}:5579/db_check/mongo",
            "interval": "15s",
            "failures_before_critical": 4
        }]
    }
}  
